		    <script type="text/javascript">
				if ("99" == "") {
					document.getElementById('otherFundLabel').style.display = 'block';
					document.getElementById('OTHER_DESIGNATION').style.display = 'block';
					requiredFields = [['FIRST_NAME','First Name'],['LAST_NAME','Last Name'],['ADDRESS_LINE_1','Street Address'],['CITY','City'],['STATE','State'],['POSTAL_CODE','Postal Code'],['PHONE','Phone'],['EMAIL','Email'],['OTHER_DESIGNATION','Fund Name']];
				}
	
            var URLSpecialDisabledFundID = "";
	
			var URLCampaignID = "4";
			var URLFundID = "";
			var hideDes1and2 = "0";
	
			if (URLSpecialDisabledFundID != "") {
				$('#designation1').hide();
				$('#designation2').hide();
				$('#designation3').hide();
	
			} else if (URLCampaignID !="") {
				var URLCampaignTypeID = (URLCampaignID == "99") ? "5" : "1";
	                        if (URLCampaignID == "99") {                            
	                            document.getElementById('designation1').value = "5";
	                            document.getElementById('designation3').style.display = 'none';                            
	                        }
				if (URLCampaignTypeID !="") {
	                                $('#designation1').find('option[value=5]').attr('selected', 'selected').end().trigger('onselect');
					$('#designation1').val(URLCampaignTypeID);
					designation1Select(URLCampaignTypeID);
					$('#designation2').val(URLCampaignID);
					designation2Select(URLCampaignID);
	
					if (URLFundID !="") {
						$('#designation3').val(URLFundID);
						document.getElementById('designation1').style.display = 'none';
						document.getElementById('designation2').style.display = 'none';
						document.getElementById('designation3').style.display = 'none';
						setSelectedFundFields(URLFundID);
					}
				}
	
			} else {
				var designation1PreSelect = document.getElementById('designation1');
				var designation1PreSelected = designation1PreSelect.options[designation1PreSelect.selectedIndex];
				designation1Select(designation1PreSelected.value);
			}
	                
	                if (hideDes1and2 == "1") {
	                    document.getElementById('designation1').style.display = 'none';
	                    document.getElementById('designation2').style.display = 'none'; 
	                    setSelectedFundFields(document.getElementById('designation3').options[0].value);
	                }
		</script>
