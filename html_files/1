	<script type="text/javascript">

	function designation1Select(designation1Selected) {
			$('#designation2').empty();
			$('#designation3').empty();
                        
			$('#des2_' + designation1Selected + '-button').clone().appendTo('#designation2');
			$('#designation3').append('<option value="">Please Select Below</option>');

            var hideDes1and2 = "0";

			//if 'Other' option is selected - hide designation2 and designation3 drop-downs and display 'Please Specify' input field
			if ('5' == designation1Selected) {
				$('#designation2').empty();
				$('#designation2').append('<option value="">Select your school or college</option>');
				$('#designation3').empty();
				$('#designation3').append('<option value="">Please Select Below</option>');

				document.getElementById('designation2').style.display = 'none';
				document.getElementById('designation3').style.display = 'none';
				document.getElementById('otherFundLabel').style.display = 'block';
				document.getElementById('OTHER_DESIGNATION').style.display = 'block';
				requiredFields = [['FIRST_NAME','First Name'],['LAST_NAME','Last Name'],['ADDRESS_LINE_1','Street Address'],['CITY','City'],['STATE','State'],['POSTAL_CODE','Postal Code'],['PHONE','Phone'],['EMAIL','Email'],['OTHER_DESIGNATION','Please Specify your fund']];

			} else if ('3' == designation1Selected) {//Momentum
				designation2Select('1000110');
				document.getElementById('designation2').style.display = 'none';
				document.getElementById('designation3').style.display = (selectElementValue == '5') ? 'none' : 'block';
				//document.getElementById('otherFundLabel').style.display = 'none';
				//document.getElementById('OTHER_DESIGNATION').style.display = 'none';
				document.getElementById('OTHER_DESIGNATION').value = '';
				requiredFields = [['FIRST_NAME','First Name'],['LAST_NAME','Last Name'],['ADDRESS_LINE_1','Street Address'],['CITY','City'],['STATE','State'],['POSTAL_CODE','Postal Code'],['PHONE','Phone'],['EMAIL','Email'],['FUND_CODE_GFT_1','Gift Designation']];

			} else if ('2' == designation1Selected) {//Library or culture
				document.getElementById('designation2').style.display = 'block';
				document.getElementById('designation3').style.display = (selectElementValue == '5') ? 'none' : 'block'; //'block';
				//document.getElementById('otherFundLabel').style.display = 'none';
				//document.getElementById('OTHER_DESIGNATION').style.display = 'none';
				document.getElementById('OTHER_DESIGNATION').value = '';
				requiredFields = [['FIRST_NAME','First Name'],['LAST_NAME','Last Name'],['ADDRESS_LINE_1','Street Address'],['CITY','City'],['STATE','State'],['POSTAL_CODE','Postal Code'],['PHONE','Phone'],['EMAIL','Email'],['FUND_CODE_GFT_1','Gift Designation']];
			} else if ('4' == designation1Selected) {//Student Life
				document.getElementById('designation2').style.display = 'block';
				document.getElementById('designation3').style.display = (selectElementValue == '5') ? 'none' : 'block'; //'block';
				//document.getElementById('otherFundLabel').style.display = 'none';
				//document.getElementById('OTHER_DESIGNATION').style.display = 'none';
				document.getElementById('OTHER_DESIGNATION').value = '';
				requiredFields = [['FIRST_NAME','First Name'],['LAST_NAME','Last Name'],['ADDRESS_LINE_1','Street Address'],['CITY','City'],['STATE','State'],['POSTAL_CODE','Postal Code'],['PHONE','Phone'],['EMAIL','Email'],['FUND_CODE_GFT_1','Gift Designation']];

			} else if ('' == designation1Selected) {
				$('#designation2').empty();
				$('#designation2').append('<option value="">Select your school or college</option>');
				$('#designation3').empty();
				$('#designation3').append('<option value="">Please Select Below</option>');
				$('#designation2').show()
				$('#designation3').css('display', (designation1Selected == '5') ? 'none' : 'block'); 
// 				$('#otherFundLabel').hide()
				$('#OTHER_DESIGNATION').css('top','-3000px').css('height','0').css('position','absolute');
				$('#OTHER_DES_BOX').val('');
				requiredFields = [['FIRST_NAME','First Name'],['LAST_NAME','Last Name'],['ADDRESS_LINE_1','Street Address'],['CITY','City'],['STATE','State'],['POSTAL_CODE','Postal Code'],['PHONE','Phone'],['EMAIL','Email'],['FUND_CODE_GFT_1','Gift Designation']];

			} else {
				$('#designation2').show();
				$('#designation3').css('display', designation1Selected == '5' ? 'none' : 'block'); //'block';
//				$('#otherFundLabel').hide();
				$('#OTHER_DESIGNATION').css('top','-3000px').css('height','0').css('position','absolute');
				$('#OTHER_DESIGNATION').value = '';
				requiredFields = [['FIRST_NAME','First Name'],['LAST_NAME','Last Name'],['ADDRESS_LINE_1','Street Address'],['CITY','City'],['STATE','State'],['POSTAL_CODE','Postal Code'],['PHONE','Phone'],['EMAIL','Email'],['FUND_CODE_GFT_1','Gift Designation']];
			}
                        
                        if (hideDes1and2 == "1") {
                            document.getElementById('designation1').style.display = 'none';
                            document.getElementById('designation2').style.display = 'none';                    
                        }                        
		}

	$('#designation1-menu').click(function() {designation1Select($('#designation1').val());});

	function designation2Select(selectElementValue) {
			$('#designation3').empty();

			if (selectElementValue != "") {
				$('#des3_' + selectElementValue + ' option').clone().appendTo('#designation3');
			} else {
				$('#designation3').append('<option value="">Please Select Below</option>');
			}

            var hideDes1and2 = "0";
			var des3 = $('#designation3');
			var l = des3.options.length;
			if (2 == l && hideDes1and2 == "0") { //only one fund for this campaign and not stern under/grad
				des3.selectedIndex = 1;
				if ('' != des3.options[1].value) {
					setSelectedFundFields(des3.options[1].value);
					document.getElementById('designation3').style.display = 'none';
				} else {
					document.getElementById('designation3').style.display = 'block';
					setSelectedFundFields('');
				}
			} else {
				setSelectedFundFields('');
				document.getElementById('designation3').style.display = 'block';
			}

                        var hideDes1and2 = "0";
                        if (hideDes1and2 == "1") {
                            document.getElementById('designation1').style.display = 'none';
                            document.getElementById('designation2').style.display = 'none';                    
                        }                        
                        
		}

	function medSchoolHandler(campaignID){//if Schools/NYU Langone Medical Center is selected or not
			if ('11' == campaignID) {
				document.getElementById('submitButton').disabled = true;
				document.getElementById('submitButton').style.display = 'none';

				document.getElementById('medSchoolNotice').style.display = 'block';
				document.getElementById('medSchoolHide').style.display = 'none';

			} else {
				document.getElementById('submitButton').disabled = false;
				document.getElementById('submitButton').style.display = 'block';

				document.getElementById('medSchoolNotice').style.display = 'none';
				document.getElementById('medSchoolHide').style.display = 'block';
			}
		}

	function medSchoolFundHandler(fundID){//if Momentum/NYU Langone Medical Center is selected or not
			if ('1000207' == fundID) {
				document.getElementById('submitButton').disabled = true;
				document.getElementById('submitButton').style.display = 'none';

				document.getElementById('medSchoolNotice').style.display = 'block';
				document.getElementById('medSchoolHide').style.display = 'none';

			} else {
				document.getElementById('submitButton').disabled = false;
				document.getElementById('submitButton').style.display = 'block';

				document.getElementById('medSchoolNotice').style.display = 'none';
				document.getElementById('medSchoolHide').style.display = 'block';
			}
		}

	function addressLine2() { //create ADDRESS_LINE_2 if data from user; otherwise do not submit this value
		var address2value = document.giftform["Add_line_2"].value;
			if (address2value!= "") {
					document.getElementById('Address2').innerHTML = "<input type='hidden' id='ADDRESS_LINE_2' name='ADDRESS_LINE_2' value=''/>";
	      			document.getElementById('ADDRESS_LINE_2').value=address2value;
			}
		}

