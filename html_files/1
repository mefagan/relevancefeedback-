	<script type="text/javascript">
		/* The following variables are set to true if the form is used with a direct link to the allocation or to the campaign.
		 * They are set to false at the beginning and then switched to true if needed. James M. 11/18/2016 
		 */
		isDirectLinkCampaign = false;
		isDirectLinkFund = false;
				
	
		/* If the form is used with a direct link (request param ID=XXXX), the normal javascript doesn't set the hidden form field
		 * merchantDefined2 which is used to transmit the allocation number to OGA. This javascript sets the value of this field in
		 * the case of a direct link. The function is called on mouseover of the html form "giftform" and also, for the case of a user
		 * not using a mouse, on focus of the FIRST_NAME field. 
		 * ServiceNow 2861294 James M. 12/21/2015
		*/
		function setDirectLinkField() {
			var fund = document.getElementById('FUND_CODE_GFT_1').value;
			var dept = document.getElementById('DEPTID_GFT_1').value;
			var program = document.getElementById('PROGRAM_CODE_GFT_1').value;
			var project = document.getElementById('PROJECT_ID_GFT_1').value;
			var merchantDefined2 = '';
			if ( (fund+dept+program+project).length > 0) {
				merchantDefined2 = fund + '-' + dept;
				if (program.length > 0) {
					merchantDefined2 = merchantDefined2 + '-' + program;
				} else {
					if (project.length > 0) {
						merchantDefined2 = merchantDefined2 + '-' + project;
					}
				}
				document.getElementById('merchantDefinedData2').value = merchantDefined2;
			}
		}
	
	</script>

